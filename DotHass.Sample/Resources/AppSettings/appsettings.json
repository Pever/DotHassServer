{
  "Start": {
    "Id": 1,
    "Name": "server-1",
    "Settings": {
      "traceoutput": "aaaa"
    },
    "AppStartup": "DotHass.Sample.StartUp",
    "ServerFactory": "DotHass.Server",

    "ShutdownTimeout": 5,
    "SuppressStatusMessages": false
  },
  "Net": {
    "BossLoopCount": 0,
    "WorkLoopCount": 0,
    "Servers": [
      {
        "Name": "game",
        "Host": "127.0.0.1",
        "Port": 7777,
        "IsSsl": false
      }
    ]
  },
  "Security": {
    "Key": "ZDLj8cWvHozBcrGG"
  },

  "ConnectionStrings": {
    "IdentityConnection": "Server=localhost;User Id=root;Password=123456;Database=dothass_identity",
    "DataConnection": "Server=localhost;User Id=root;Password=123456;Database=dothass_data",
    "LogConnection": "Server=localhost;User Id=root;Password=123456;Database=dothass_log"
  },
  "Repository": {
    "JsonConfig": {
      "path": "Resources/Configs"
    },
    "redis": [
      {
        "key": "redisConnection",
        "connectionString": "localhost:6379,allowAdmin=true",
        "database": 1
      }
    ],
    "cacheManagers": [
      {
        "handles": [
          {
            "knownType": "Redis",
            "key": "redisConnection",
            "isBackplaneSource": false
          }
        ],
        "loggerFactory": {
          "knownType": "Microsoft"
        },
        "maxRetries": 1000,
        "name": "default",
        "retryTimeout": 100,
        "serializer": {
          "knownType": "Json"
        },
        "updateMode": "Up"
      }
    ]
  },

  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File": {
      "PathFormat": "Logs/log-{Date}.txt",
      "Json": false,
      "LogLevel": {
        "Default": "Error"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}